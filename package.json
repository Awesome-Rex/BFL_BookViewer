{
  "name": "bfl-book-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^18.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-pdf": "^5.7.2",
    "react-scripts": "5.0.0",
    "snowpack": "^3.8.8",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "webpack-start": "react-scripts start",
    "webpack-build": "react-scripts build",
    "webpack-test": "react-scripts test",
    "webpack-eject": "react-scripts eject",
    "parcel-cleanup": "rd /s /q \".cache\" && rd /s /q \"dist\"",
    "parcel-dev": "npm run cleanup && parcel src/index.html",
    "parcel-build": "parcel build src/index/html",
    "snowpack-start": "snowpack dev",
    "dart-sass-watch": "npx sass --watch src/scss:dist/css",
    "svgo-dev": "svgo -f ./src/assets/svg -o ./src/assets/images",
    "prettier-all": "npx prettier --write .",
    "prettier-specific": "prettier --write \"./**/*.{js,jsx,json,ts,tsx,scss,css,html}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.0",
    "@types/react-pdf": "^5.0.9",
    "@types/three": "^0.139.0",
    "copy-webpack-plugin": "^10.2.4",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.50.0"
  }
}
